{
  "name": "investments-chart",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "antd": "^4.0.3",
    "apexcharts": "^3.17.0",
    "axios": "^0.19.2",
    "history": "^4.9.0",
    "moment": "^2.24.0",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-apexcharts": "^1.3.6",
    "react-awesome-styled-grid": "^3.0.4",
    "react-dom": "^16.9.0",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "recharts": "^1.8.5",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "release-it": "^12.4.3",
    "styled-components": "^5.0.1"
  },
  "scripts": {
    "install:all": "yarn && cd ./src/server && npm install",
    "start": "concurrently --kill-others \"yarn run proxy:start\" \"yarn run start:dev\"",
    "start:dev": "cross-env REACT_APP_ENV=dev react-scripts start",
    "build:dev": "cross-env REACT_APP_ENV=dev react-scripts build",
    "build:prod": "cross-env REACT_APP_ENV=prod react-scripts build",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:watch": "react-scripts test --watchAll",
    "test": "react-scripts test --watchAll=false",
    "proxy:start": "node ./src/server/index.js",
    "release": "release-it",
    "lint": "eslint ./src --fix",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.3",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.8.6",
    "redux-mock-store": "^1.5.3"
  }
}
